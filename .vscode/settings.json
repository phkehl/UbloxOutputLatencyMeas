{
    //"C_Cpp.intelliSenseEngine": "Tag Parser",
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    // Exclude files and folders from explorer and IntelliSense (it automatically excludes .git)
    "files.exclude": {
        //"build": true,
        "**/*~": true,
        "build/**": true,
        "build-*/**": true,
        ".vstags": true,
        "old/**": true,
        ".history/**": true
    },
    // Exclude things from search (this automatically uses files.exclude + what you add here)
    "search.exclude": {
    },
    // Exclude some things from the files watcher
    "files.watcherExclude": {
        "**/build/**": true
    },
    "files.associations": {
    },
    
